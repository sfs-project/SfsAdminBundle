<!--BEGIN TOPBAR-->
<div class="top-bar">
	<nav class="navbar navbar-default pr-16" role="navigation">
		<ul class="nav navbar-nav">
			<li>
				<a href="#menu-toggle" id="menu-toggle" class="">
					<i class="fa fa-bars fa-lg"></i>
				</a>
			</li>
		</ul>
		{% block breadcrumb_list %}
			{{ knp_menu_render('breadcrumb', {'template': 'SfsAdminBundle:Menu:knp_breadcrumb.html.twig', 'allow_safe_labels': true}) }}
		{% endblock %}

		{% block breadcrumb_page_title %}
			{% include 'SfsAdminBundle:Core:page_title.html.twig' %}
		{% endblock %}

		{{ knp_menu_render('topbar', {'template': 'SfsAdminBundle:Menu:knp_topbar.html.twig', 'allow_safe_labels': true }) }}
	</nav>
</div>
<!--END TOPBAR-->
